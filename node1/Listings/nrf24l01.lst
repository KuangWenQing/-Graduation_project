  17          /*****************SPI时序函数********************
  18          读数据时，byte写0
  19          
  20          其实不论读写数据，该函数都会返回byte，
  21          只是配置成写数据MISO上的电平不会变。
  22          ************************************************/
  23          static u8 SPI_RW(u8 byte)
  24          {
  25   1        u8 bit_ctr;
  26   1        for(bit_ctr=0; bit_ctr<8; bit_ctr++)
  27   1        {
  28   2          MOSI = (byte & 0x80);   //MCU to NRF24L01 
  29   2          byte = (byte << 1);    
  30   2          
  31   2          SCK = 1;     
  32   2          
  33   2          byte |= MISO;               //NRF24L01 to MCU     
  34   2          
  35   2          SCK = 0;                                
  36   2        }
  37   1        return(byte);                              
  38   1      }
